{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./web_app/static"
    },
    "include": [
        "web_app/src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}