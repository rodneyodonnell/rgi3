    "decay_lr": true,
    "min_lr": 5e-05,
    "lr_decay_iters": 5000,
    "warmup_iters": 100,
    "weight_decay": 0.01,
    "beta1": 0.9,
    "beta2": 0.95,
    "grad_clip": 1.0,
    "dtype": "float32",
    "eval_iters": 20,
    "log_interval": 50,
    "eval_interval": 50,
    "early_stop_patience": 20
